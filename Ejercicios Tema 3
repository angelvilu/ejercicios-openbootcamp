public class Main {
    public static void main(String[] args) {
        // PARTE 1
        suma(1,4,8);

        // PARTE 2
        Coche miCoche = new Coche();
        miCoche.puertas = 5;
        
        System.out.println(miCoche.puertas);
    }

    //PARTE 1
    public static void suma(int a, int b, int c){
        int resultado;
        resultado = a + b + c;

        System.out.println(resultado);

    }
}
// PARTE 2
class Coche {
    int puertas;
    
    public static int masPuertas(int a) {
        return a++;
    }
}
